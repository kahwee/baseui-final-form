{
  "name": "baseui-final-form",
  "version": "2.0.5",
  "main": "./dist-node-cjs/index.js",
  "module": "./dist-node-esm/index.js",
  "browser": {
    "./dist-node-cjs/index.js": "./dist-browser-cjs/index.js",
    "./dist-node-esm/index.js": "./dist-browser-esm/index.js"
  },
  "files": [
    "dist-browser-cjs",
    "dist-browser-esm",
    "dist-node-cjs",
    "dist-node-esm",
    "src",
    "!**__tests__**",
    "!stories.js"
  ],
  "author": "KahWee Teng <tengkahwee@gmail.com>",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "transpile": "npm run clean && cup build",
    "prepublishOnly": "npm run build && node -e \"const fs = require('fs'); const pkg = JSON.parse(fs.readFileSync('./package.json', {encoding: 'utf8'})); delete pkg.engines; fs.writeFileSync('package.json', JSON.stringify(pkg, null, 2)); console.log(pkg)\"",
    "build": "npm run transpile",
    "lint": "eslint src/*",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -c .storybook -o storybook-static",
    "test": "jest",
    "cover": "jest --coverage",
    "flow": "flow check",
    "flow-fast": "flow"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "@emotion/core": "^10.0.14",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@testing-library/react": "^8.0.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.5",
    "baseui": "8.7.0",
    "create-universal-package": "^4.0.0",
    "emotion-theming": "^10.0.14",
    "eslint": "^6.0.1",
    "eslint-config-fusion": "^6.0.1",
    "eslint-plugin-cup": "^2.0.0",
    "eslint-plugin-flowtype": "^3.12.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.10.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0",
    "final-form": "^4.18.2",
    "final-form-arrays": "^3.0.1",
    "flow-bin": "0.100.0",
    "global": "^4.4.0",
    "husky": "^3.0.0",
    "jest": "^24.7.1",
    "jest-extended": "^0.11.2",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "3.1.0",
    "rimraf": "^2.6.3",
    "storybook-readme": "^5.0.5",
    "styletron-engine-atomic": "^1.4.0",
    "styletron-react": "^5.2.0"
  },
  "peerDependencies": {
    "baseui": ">= 7.5.0 < 9",
    "final-form": "^4.16.1",
    "final-form-arrays": "^1.1.2",
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17",
    "react-final-form": ">= 6.3.0 < 7",
    "react-final-form-arrays": ">= 3.1.0"
  },
  "keywords": [
    "react",
    "reactjs",
    "final-form",
    "react-final-form",
    "forms"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/setup-tests.js"
    ],
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist*"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/setup-tests.js",
      "!**/stories.js",
      "!**/node_modules/**",
      "!**/dist/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kahwee/baseui-final-form.git"
  },
  "license": "MIT",
  "dependencies": {
    "react-select": "^3.0.4"
  }
}
